{
	"name": "@mfyuu/utils",
	"type": "module",
	"version": "0.2.0",
	"description": "Opinionated collection of common JavaScript / TypeScript utils by @mfyuu",
	"author": "mfyuu",
	"license": "MIT",
	"homepage": "https://github.com/mfyuu/utils",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mfyuu/utils.git"
	},
	"bugs": {
		"url": "https://github.com/mfyuu/utils/issues"
	},
	"keywords": [
		"utils"
	],
	"sideEffects": false,
	"exports": {
		".": "./dist/index.mjs"
	},
	"main": "dist/index.mjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.mts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"prepare": "node -e \"if(process.env.CI !== 'true') { require('child_process').execSync('lefthook install', {stdio: 'inherit'}) }\"",
		"build": "unbuild",
		"dev": "unbuild --stub",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"lint-fix": "biome lint --write .",
		"check": "biome check --write .",
		"prepublishOnly": "nr build",
		"start": "tsx src/index.ts",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "nr build && changeset publish"
	},
	"devDependencies": {
		"@antfu/ni": "^28.0.0",
		"@biomejs/biome": "2.3.10",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.6",
		"@mfyuu/biome-config": "^1.0.0",
		"@types/node": "^24.3.0",
		"lefthook": "^2.0.0",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2",
		"unbuild": "^3.6.1",
		"vite": "^7.1.3",
		"vitest": "^4.0.0"
	},
	"packageManager": "pnpm@10.25.0"
}
